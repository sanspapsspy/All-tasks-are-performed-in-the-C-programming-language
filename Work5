#include <stdio.h>

// Функция быстрого возведения в степень
unsigned long power(int base, int exponent) {
    unsigned long result = 1;
    while(exponent > 0) {
        if (exponent % 2 != 0)
            result *= base;
        
        base *= base;
        exponent /= 2;
    }
    
    return result;
}

int main() {
    int number, degree;
    printf("Введите число: ");
    scanf("%d", &number);
    printf("Введите степень: ");
    scanf("%d", &degree);
    
    // Проверяем, что степень неотрицательная
    if(degree >= 0) {
        unsigned long result = power(number, degree);
        printf("%d в степени %d равно %lu\\n", number, degree, result);
    } else {
        printf("Степень должна быть неотрицательной.\\n");
    }
    
    return 0;
}
