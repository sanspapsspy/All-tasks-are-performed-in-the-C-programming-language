#include <stdio.h>
#include <string.h>

// Реализация функции сравнения строк, аналогичной strcmp
int custom_strcmp(const char* str1, const char* str2) {
    while (*str1 && *str2) {
        if (*(unsigned char*)str1 != *(unsigned char*)str2) {
            return *(unsigned char*)str1 - *(unsigned char*)str2;
        }
        str1++;
        str2++;
    }
    // Проверяем оставшиеся символы
    return *(unsigned char*)str1 - *(unsigned char*)str2;
}

// Демонстрационная функция
int main() {
    const char* strings[] = {"apple", "banana", "Apple", "orange", "Banana"};

    for (int i = 0; i < sizeof(strings)/sizeof(strings[0]) - 1; i++) {
        int cmp_result = custom_strcmp(strings[i], strings[i+1]);
        if (cmp_result < 0) {
            printf("\\"%s\\" меньше \\"%s\\"\\n", strings[i], strings[i+1]);
        } else if (cmp_result > 0) {
            printf("\\"%s\\" больше \\"%s\\"\\n", strings[i], strings[i+1]);
        } else {
            printf("\\"%s\\" совпадает с \\"%s\\"\\n", strings[i], strings[i+1]);
        }
    }

    return 0;
}
